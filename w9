//Write a C program to input length in centimeter and convert it to meter and kilometer
#include <stdio.h>
int main() 
{
    float centimeters;
    float meters;
    float kilometers;
    // Prompt the user to enter the length in centimeters
    printf("Enter the length in centimeters");
    // Read the input from the user
    scanf("%f", &centimeters);
    // Convert centimeters to meters (1 meter = 100 centimeters)
    meters = centimeters / 100.0;
    // Convert centimeters to kilometers (1 kilometer = 100,000 centimeters)
    kilometers = centimeters / 100000.0;
    // Display the converted lengths
    printf("Length in meters: %.2f m\n", meters);
    printf("Length in kilometers: %.5f km\n", kilometers);
}
