//program to find total marks and percantage
#include<stdio.h>
main()
{
	int i,marks[6],sum=0,per=0;
	for(i=0;i<6;i++)
	{
		printf("enter sub%d marks\n",i+1);
		scanf("%d",&marks);
	}
	for(i=0;i<6;i++)
	sum=marks[i]+sum;
	printf("%d is total\n",sum);
	per=(sum/600)*100;
	printf(".2%d is percantage\n",per);
}
