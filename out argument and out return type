//Funtion without argument and without return type
#include<stdio.h>
void addition(); //function declaration
main()
{
	addition();
	addition();
	addition(); //function calling
}
void addition ()  //function definate
{
	int a, b, c;
	printf("enter a and b values");
	scanf("%d%d",&a,&b);
	c=a+b;
	printf("the sum is %d",c);
}
